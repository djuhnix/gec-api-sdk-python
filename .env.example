# Environment variables for GEC API Member Import
# Copy this file to .env and fill in your values

# GEC API Configuration
GEC_API_HOST=http://localhost
GEC_API_TOKEN=your_jwt_token_here

# Google Sheets Authentication (optional, only needed for Google Sheets imports)
# For OAuth2: Path to credentials.json from Google Cloud Console
# For Service Account: Path to service account JSON key
GOOGLE_CREDENTIALS_PATH=path/to/credentials.json

